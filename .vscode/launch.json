{
    "version": "0.1.0",
    "configurations": [
        {
            "name": "Launch index.html (disable sourcemaps)",
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "smartStep": true,
            "file": "${workspaceRoot}/docs/index.html",
            "sourceMapPathOverrides": {
                "webpack:///./~/*": "${workspaceRoot}/game/node_modules/*",
                "webpack:///./src/*": "${workspaceRoot}/game/src/*"
            }
        }
    ]
}
